{
	"name": "georgringer/mailtonews",
	"type": "typo3-cms-extension",
	"description": "Most simple way to write news entries, by just sending a mail to a specific email address",
	"license": ["GPL-2.0+"],
	"support": {
		"issues": "https://github.com/pixelant/mailtonews"
	},
	"version": "2.3.0",
	"replace": {
		"mailtonews": "*"
	},
	"require": {
		"typo3/cms-core": "6.0.0 - 8.9.99",
		"php-imap/php-imap": "^3.0"
	},
	"autoload": {
		"psr-4": {
			"GeorgRinger\\Mailtonews\\": "Classes/"
		}
	},
	"config": {
		"vendor-dir": "Resources/Private/Php"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}
